{
    "manifest_version": 3,
    "name": "Vibes Tracker",
    "version": "1.0",
    "description": "An engine to analyze emotions on faces detected in Google Meet.",
    "author": "Anik Ghosh",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "content_scripts": [
        {
            "matches": ["*://meet.google.com/*"],
            "js": ["content.js","face-api.min.js"]
        }
    ],
    "web_accessible_resources": [
        {
          "resources": ["models/*"],
          "matches": ["<all_urls>"]
        }
    ]
}
